<!-- _meta/attr/view/classes -->
<%
const names = model.get(attr);
if (Array.isArray(names)) {
  let result = '';
  for (const name of names) {
    const cls = model.class.meta.getClass(name);
    result += cls
      ? `<div title="${name}">${_escape(cls.title)}</div>`
      : `<div>${name}</div>`;
  }
  model.setDisplayValue(attr, result);
}
%>
<%- include(_view.get('_meta/attr/static'), {formatted: true}) %>