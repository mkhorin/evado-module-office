<!-- office/./_meta/attr/view/user -->
<%
const value = model.get(attr);
if (value) {
    const text = model.related.getTitle(attr);
    const url = _module.getParam('userUpdateUrl');
    url ? model.setDisplayValue(attr, _format(_url(url) + value, 'url', {target: '_blank', text}))
        : model.setDisplayValue(attr, text);
}
%>
<%- include(_view.get('_meta/attr/static')) %>