<!-- _meta/list/sort -->
<%
data = {
    jam: 'SortList',
    key: meta.class.getKey(),
    multiple: true,
    columns: extraData.columns,
    url: _controller.getOriginalUrl(),
    update: `office/model/update?c=${meta.class.id}`,
    pageSize: 50,
    ...data
};
%>
<div class="jmodal-scroll-container box" data-tab="Sort" data-title="<%= blocks.title %>">
    <div class="box-header with-border scroll-header"></div>
    <div class="scroll-body box-body">
        <div class="data-grid" data-jam="<%- data.jam %>" data-params="<%= JSON.stringify(data) %>">
            <div class="list-commands clearfix">
                <button data-command="saveClose" class="btn btn-primary" type="button" title="Save and close">
                    <span class="glyphicon glyphicon-off"></span>
                </button>
                <div class="btn-set">
                    <button data-command="up" class="btn btn-default" type="button">
                        <span class="glyphicon glyphicon-arrow-up"></span>
                    </button>
                    <button data-command="down" class="btn btn-default" type="button">
                        <span class="glyphicon glyphicon-arrow-down"></span>
                    </button>
                </div>
                <% if (data.view) { %>
                <button data-command="view" class="btn btn-default" type="button" title="View">
                    <span class="glyphicon glyphicon-eye-open"></span>
                </button>
                <% } if (data.update && security.access.canUpdate()) { %>
                <button data-command="update" class="btn btn-warning btn-light" type="button" title="Edit">
                    <span class="glyphicon glyphicon-pencil"></span>
                </button>
                <% } %>
                <div class="btn-set">
                    <button data-command="reload" class="btn btn-default" type="button" title="Reload">
                        <span class="fa fa-redo-alt"></span>
                    </button>
                </div>
            </div>
            <div class="table-responsive">
                <table class="data-grid-table table table-bordered table-striped table-condensed">
                    <thead></thead>
                    <tbody></tbody>
                </table>
            </div>
            <%- include(_view.get('_part/list/bottom')) %>
        </div>
    </div>
    <div class="list-loader overlay"><i class="fa fa-spinner fa-spin"></i></div>
</div>